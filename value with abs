#include <stdio.h>

int main() {
    int year1;
    int year2;
    int age;
    
    printf("enter the age\n", year1);
    scanf("%d",&year1);
    
    printf("enter the age\n", year2);
    scanf("%d",&year2);
    
    age = year1 - year2;
    printf("%d\n", age);
    age = abs(age);
    printf("%d\n",age);
    
    return 0;
}
