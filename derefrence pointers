#include <stdio.h>

int main() {
    int tuna= 19;
    printf("address \t name\t value\n");
    printf("%p\t %s\t %d\n", &tuna,"tuna", tuna);
    
    int * ptuna = &tuna;
    
    printf("%p\t %s\t %d\n",ptuna, "tuna", tuna);
    printf("%p\t %s\t %d\n", &ptuna, "tuna", tuna);
    
    printf("\n *ptuna : %d\n", *ptuna);
    *ptuna = 45;
    printf("\n *ptuna : %d\n", *ptuna);
    return 0;
}
